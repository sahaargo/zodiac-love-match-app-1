                      <img src={image} alt={`ржЖржкржирж╛рж░ рж╣рж╛рждрзЗрж░ рждрж╛рж▓рзБ ${index + 1}`} className="rounded-xl shadow-md w-full h-auto object-cover max-h-96" />
