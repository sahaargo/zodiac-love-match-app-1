            </div>

            <Button
              onClick={checkMatch}
              className="w-full bg-gradient-to-r from-blue-500 to-indigo-600 hover:from-blue-600 hover:to-indigo-700 text-white font-bold py-3 px-4 rounded-lg shadow-lg transition duration-300 ease-in-out transform hover:-translate-y-1 hover:scale-105 mt-6"
            >
              ржорж┐рж▓ ржжрзЗржЦрзБржи
            </Button>

            {matchResult && (
              <div className="mt-4 p-4 bg-blue-100 text-blue-900 rounded-lg border-2 border-blue-300 shadow-md">
                <h3 className="text-xl font-bold text-center mb-2">{matchResult.level}</h3>
                <p className="mb-2">**ржмрж░рзНржгржирж╛:** {matchResult.description}</p>
                <p className="mb-2">**рж╢ржХрзНрждрж┐рж░ ржжрж┐ржХ:** {matchResult.strengths}</p>
                <p>**ржЪрзНржпрж╛рж▓рзЗржЮрзНржЬ:** {matchResult.challenges}</p>
              </div>
            )}
            {palmUploadPrompt && (
              <div className="text-yellow-700 bg-yellow-100 p-3 rounded-md border border-yellow-200 mt-4 text-center">
                {palmUploadPrompt}
              </div>
            )}
          </CardContent>
        </Card>
      )}

      {currentView === 'personal' && (
        <Card className="rounded-2xl shadow-xl border border-gray-200 bg-white p-6 relative z-10"> {/* Added z-10 */}
          <CardContent className="space-y-6 pt-0">
            <h2 className="text-2xl font-bold text-gray-800 border-b-2 border-purple-200 pb-2 mb-4">ржЖржорж╛рж░ ржмрзНржпржХрзНрждрж┐ржЧржд рж░рж╛рж╢рж┐ржлрж▓</h2>
            
            {/* Your Personal Details Input Section */}
            <div>
              <label className="block text-sm font-medium text-gray-700 mb-1">ржЖржкржирж╛рж░ ржирж╛ржо:</label>
              <Input
                type="text"
                placeholder="ржЖржкржирж╛рж░ ржирж╛ржо рж▓рж┐ржЦрзБржи"
                value={yourName}
                onChange={(e) => setYourName(e.target.value)}
                className="w-full p-3 border border-gray-300 rounded-lg focus:border-purple-500 focus:ring-purple-500"
              />
            </div>

            <div className={`relative p-3 rounded-lg overflow-hidden ${yourSign ? 'input-background-animation rashi-your' : ''}`}>
              <label className="block text-sm font-medium text-gray-700 mb-1">ржЖржкржирж╛рж░ рж░рж╛рж╢рж┐:</label>
              <Select onValueChange={setYourSign} value={yourSign}>
                <SelectTrigger className="w-full rounded-lg border border-gray-300 px-4 py-2 text-gray-700 shadow-sm focus:border-purple-500 focus:ring-purple-500">
                  <SelectValue placeholder="рж░рж╛рж╢рж┐ ржирж┐рж░рзНржмрж╛ржЪржи ржХрж░рзБржи" />
                </SelectTrigger>
                <SelectContent className="rounded-lg shadow-lg border border-gray-200 bg-white z-10">
                  {Object.keys(compatibility).map((sign) => (
                    <SelectItem key={sign} value={sign} className="px-4 py-2 hover:bg-gray-100 cursor-pointer">
                      {sign}
                    </SelectItem>
                  ))}
                </SelectContent>
              </Select>
            </div>

            <div>
              <label className="block text-sm font-medium text-gray-700 mb-1">ржЬржирзНржо рждрж╛рж░рж┐ржЦ:</label>
              <Input
                type="date"
                value={yourBirthDate}
                onChange={(e) => setYourBirthDate(e.target.value)}
                className="w-full p-3 border border-gray-300 rounded-lg focus:border-purple-500 focus:ring-purple-500"
              />
            </div>

            <div>
              <label className="block text-sm font-medium text-gray-700 mb-1">ржЬржирзНржо рж╕ржоржпрж╝:</label>
              <Input
                type="time"
                value={yourBirthTime}
                onChange={(e) => setYourBirthTime(e.target.value)}
                className="w-full p-3 border border-gray-300 rounded-lg focus:border-purple-500 focus:ring-purple-500"
              />
            </div>

            <div>
              <label className="block text-sm font-medium text-gray-700 mb-1">ржЬржирзНржо рж╕рзНржерж╛ржи:</label>
              <Input
                type="text"
                placeholder="ржпрзЗржоржи: ржврж╛ржХрж╛"
                value={yourBirthPlace}
                onChange={(e) => setYourBirthPlace(e.target.value)}
                className="w-full p-3 border border-gray-300 rounded-lg focus:border-purple-500 focus:ring-purple-500"
              />
            </div>

            {/* Calculate Zodiac Sign Button */}
            <Button
              onClick={calculateZodiacSignFromBirthDetails}
              disabled={isCalculatingSign || !yourBirthDate || !yourBirthTime || !yourBirthPlace}
              className="w-full bg-gradient-to-r from-green-500 to-teal-600 hover:from-green-600 hover:to-teal-700 text-white font-bold py-3 px-4 rounded-lg shadow-lg transition duration-300 ease-in-out transform hover:-translate-y-1 hover:scale-105 mt-4"
            >
              {isCalculatingSign ? (
                <span className="flex items-center justify-center">
                  <Loader2 className="mr-2 h-5 w-5 animate-spin" />
                  рж░рж╛рж╢рж┐ ржмрзЗрж░ ржХрж░рж╛ рж╣ржЪрзНржЫрзЗ...
                </span>
              ) : (
                "ржЬржирзНржо ржмрж┐ржмрж░ржг ржерзЗржХрзЗ рж░рж╛рж╢рж┐ ржмрзЗрж░ ржХрж░рзБржи"
              )}
            </Button>

            {signCalculationError && (
              <div className="text-red-500 text-center mt-2 p-2 bg-red-50 rounded-md border border-red-200">
                {signCalculationError}
              </div>
            )}

            {/* Display Your Selected Sign with Icon */}
            <div className="p-4 bg-purple-50 rounded-lg border border-purple-200 text-purple-800 flex items-center justify-between">
              <p className="text-lg font-semibold">
                {yourName ? `${yourName}-ржПрж░ ржирж┐рж░рзНржмрж╛ржЪрж┐ржд рж░рж╛рж╢рж┐:` : "ржЖржкржирж╛рж░ ржирж┐рж░рзНржмрж╛ржЪрж┐ржд рж░рж╛рж╢рж┐:"} {yourSign || "ржирж┐рж░рзНржмрж╛ржЪржи ржХрж░рж╛ рж╣ржпрж╝ржирж┐"}
              </p>
              {yourSign && rashiEmojis[yourSign] && (
                <span className="text-4xl leading-none ml-2 transform transition-transform duration-300 hover:scale-125">
                  {rashiEmojis[yourSign]}
                </span>
              )}
            </div>

            {/* Display Your Birth Details (from state) */}
            <div className="p-4 bg-gray-50 rounded-lg border border-gray-200 text-gray-800">
              <h3 className="text-lg font-semibold mb-2">ржЖржкржирж╛рж░ ржЬржирзНржо ржмрж┐ржмрж░ржг:</h3>
              <p>рждрж╛рж░рж┐ржЦ: {yourBirthDate || "ржжрзЗржУржпрж╝рж╛ рж╣ржпрж╝ржирж┐"}</p>
              <p>рж╕ржоржпрж╝: {yourBirthTime || "ржжрзЗржУржпрж╝рж╛ рж╣ржпрж╝ржирж┐"}</p>
              <p>рж╕рзНржерж╛ржи: {yourBirthPlace || "ржжрзЗржУржпрж╝рж╛ рж╣ржпрж╝ржирж┐"}</p>
            </div>

            {/* Save Data Button */}
            <Button
              onClick={saveUserData}
              disabled={!yourName || !yourBirthDate || !yourBirthTime || !yourBirthPlace || !yourSign || !isAuthReady}
              className="w-full bg-gradient-to-r from-teal-500 to-green-600 hover:from-teal-600 hover:to-green-700 text-white font-bold py-3 px-4 rounded-lg shadow-lg transition duration-300 ease-in-out transform hover:-translate-y-1 hover:scale-105 mt-4"
            >
              ржЖржорж╛рж░ рждржерзНржп рж╕ржВрж░ржХрзНрж╖ржг ржХрж░рзБржи
            </Button>

            {/* Notification Toggle and Test Button */}
            <div className="flex items-center justify-between p-4 bg-gray-100 rounded-lg border border-gray-200 mt-4">
              <div className="flex items-center space-x-2">
                {notificationEnabled ? <Bell className="h-6 w-6 text-green-600" /> : <BellOff className="h-6 w-6 text-red-600" />}
                <label htmlFor="notification-toggle" className="text-lg font-semibold text-gray-800">ржжрзИржирж┐ржХ ржирзЛржЯрж┐ржлрж┐ржХрзЗрж╢ржи</label>
              </div>
              <Switch
                id="notification-toggle"
                checked={notificationEnabled}
                onCheckedChange={async (checked) => {
                  if (checked) {
                    await requestNotificationPermission();
                  } else {
                    setNotificationEnabled(false);
                    setNotificationMessage("ржирзЛржЯрж┐ржлрж┐ржХрзЗрж╢ржи ржмржирзНржз ржХрж░рж╛ рж╣ржпрж╝рзЗржЫрзЗред");
                    setTimeout(() => setNotificationMessage(""), 3000);
                  }
                }}
              />
            </div>
            {notificationPermissionStatus !== 'granted' && (
              <p className="text-sm text-red-500 mt-2 text-center">
                ржирзЛржЯрж┐ржлрж┐ржХрзЗрж╢ржи ржкрзЗрждрзЗ рж╣рж▓рзЗ ржмрзНрж░рж╛ржЙржЬрж╛рж░рзЗ ржЕржирзБржорждрж┐ ржжрж┐рждрзЗ рж╣ржмрзЗред
              </p>
            )}
            <Button
              onClick={() => sendTestNotification(yourSign)}
              disabled={!yourSign || !notificationEnabled || notificationPermissionStatus !== 'granted'}
              className="w-full bg-gradient-to-r from-yellow-500 to-orange-600 hover:from-yellow-600 hover:to-orange-700 text-white font-bold py-3 px-4 rounded-lg shadow-lg transition duration-300 ease-in-out transform hover:-translate-y-1 hover:scale-105 mt-2"
            >
              ржЯрзЗрж╕рзНржЯ ржирзЛржЯрж┐ржлрж┐ржХрзЗрж╢ржи ржкрж╛ржарж╛ржи
            </Button>
            <p className="text-xs text-gray-500 mt-2 text-center">
              **ржмрж┐рж╢рзЗрж╖ ржжрзНрж░рж╖рзНржЯржмрзНржп:** ржПржЗ ржЕрзНржпрж╛ржкржЯрж┐ ржмрзНрж░рж╛ржЙржЬрж╛рж░рзЗ ржЦрзЛрж▓рж╛ ржирж╛ ржерж╛ржХрж▓рзЗ ржжрзИржирж┐ржХ ржирзЛржЯрж┐ржлрж┐ржХрзЗрж╢ржи ржкрж╛ржарж╛ржирзЛ рж╕ржорзНржнржм ржиржпрж╝ред ржирж┐ржпрж╝ржорж┐ржд ржирзЛржЯрж┐ржлрж┐ржХрзЗрж╢ржирзЗрж░ ржЬржирзНржп ржЕрзНржпрж╛ржкржЯрж┐ ржмрзНржпрж╛ржХржЧрзНрж░рж╛ржЙржирзНржбрзЗ ржЦрзЛрж▓рж╛ рж░рж╛ржЦрждрзЗ рж╣ржмрзЗ ржЕржержмрж╛ ржПржХржЯрж┐ рж╕рж╛рж░рзНржнрж╛рж░-рж╕рж╛ржЗржб рж╕ржорж╛ржзрж╛ржи ржкрзНрж░ржпрж╝рзЛржЬржиред
            </p>

            {notificationMessage && (
              <div className="mt-4 p-3 bg-blue-100 text-blue-800 rounded-md border border-blue-200 text-center">
                {notificationMessage}
              </div>
            )}

            {/* Daily Rashi Update Section */}
            <div className="space-y-3 mt-4">
              <Button
                onClick={fetchDailyRashiUpdate}
                disabled={isFetchingDailyRashi || !yourSign}
                className="w-full bg-gradient-to-r from-purple-500 to-pink-600 hover:from-purple-600 hover:to-pink-700 text-white font-bold py-3 px-4 rounded-lg shadow-lg transition duration-300 ease-in-out transform hover:-translate-y-1 hover:scale-105"
              >
                {isFetchingDailyRashi ? (
                  <span className="flex items-center justify-center">
                    <Loader2 className="mr-2 h-5 w-5 animate-spin" />
                    ржжрзИржирж┐ржХ рж░рж╛рж╢рж┐ржлрж▓ ржЖржирж╛ рж╣ржЪрзНржЫрзЗ...
                  </span>
                ) : (
                  "ржЖржЬржХрзЗрж░ ржжрзИржирж┐ржХ рж░рж╛рж╢рж┐ржлрж▓ ржжрзЗржЦрзБржи"
                )}
              </Button>

              {dailyRashiError && (
                <div className="text-red-500 text-center mt-2 p-2 bg-red-50 rounded-md border border-red-200">
                  {dailyRashiError}
                </div>
              )}

              {dailyRashiUpdate && (
                <div className="mt-4 p-4 bg-yellow-50 text-yellow-800 rounded-lg border border-yellow-200 shadow-md">
                  <h3 className="text-xl font-semibold mb-2">ржЖржЬржХрзЗрж░ рж░рж╛рж╢рж┐ржлрж▓ ({yourSign}):</h3>
                  <p className="whitespace-pre-wrap">{dailyRashiUpdate}</p>
                </div>
              )}
            </div>
          </CardContent>
        </Card>
      )}
    </div>
  );
}
