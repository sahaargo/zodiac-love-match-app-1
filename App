              <Input
                id="your-palm-upload"
                type="file"
                accept="image/*"
                multiple // Allow multiple file selection
                onChange={(e) => handlePalmImageUpload(e, false)}
                className="w-full p-2 border border-gray-300 rounded-md focus:border-blue-500 focus:ring-blue-500"
              />
              {yourPalmImageError && (
                <p className="text-red-500 text-sm mt-2">{yourPalmImageError}</p>
              )}
              <p className="text-xs text-gray-500">
                рж╣рж╛рждрзЗрж░ рждрж╛рж▓рзБрж░ ржкрж░рж┐рж╖рзНржХрж╛рж░ ржЫржмрж┐ ржжрж┐ржиред ржлрзНрж▓рзНржпрж╛рж╢ ржЕржи ржХрж░рзЗ рждрзБрж▓рзБржиред ржХржмрзНржЬрж┐ ржерзЗржХрзЗ ржЖржЩрзБрж▓ ржкрж░рзНржпржирзНржд ржжрзЗржЦрж╛ ржпрж╛рзЯ ржПржоржи ржЫржмрж┐ ржжрж┐ржиред
              </p>

              {yourPalmImages.length > 0 && ( // Iterate over images
                <div className="grid grid-cols-1 sm:grid-cols-2 gap-4 mt-4">
                  {yourPalmImages.map((image, index) => (
