        strengths: "ржорзАржи ржХрзБржорзНржнржХрзЗ ржЖржмрзЗржЧрж┐ржХ рж╕ржорж░рзНржержи ржжрж┐рждрзЗ ржкрж╛рж░рзЗ, ржЖрж░ ржХрзБржорзНржн ржорзАржиржХрзЗ ржирждрзБржи ржзрж╛рж░ржгрж╛ ржжрж┐рждрзЗ ржкрж╛рж░рзЗред",
        challenges: "ржорзАржирзЗрж░ рж╕ржВржмрзЗржжржирж╢рзАрж▓рждрж╛ ржХрзБржорзНржнрзЗрж░ ржХрж╛ржЫрзЗ ржЕрждрж┐рж░рж┐ржХрзНржд ржоржирзЗ рж╣рждрзЗ ржкрж╛рж░рзЗред",
      },
    }
  };

  // Zodiac sign to emoji mapping
  const rashiEmojis = {
    "ржорзЗрж╖": "тЩИ",
    "ржмрзГрж╖": "тЩЙ",
    "ржорж┐ржерзБржи": "тЩК",
    "ржХрж░рзНржХржЯ": "тЩЛ",
    "рж╕рж┐ржВрж╣": "тЩМ",
    "ржХржирзНржпрж╛": "тЩН",
    "рждрзБрж▓рж╛": "тЩО",
    "ржмрзГрж╢рзНржЪрж┐ржХ": "тЩП",
    "ржзржирзБ": "тЩР",
    "ржоржХрж░": "тЩС",
    "ржХрзБржорзНржн": "тЩТ",
    "ржорзАржи": "тЩУ"
  };

  // --- Animation Logic ---
  // Use useEffect to manage body class for full-screen background animation
  useEffect(() => {
    // Remove all existing zodiac classes from body
    document.body.classList.remove(...Object.keys(rashiEmojis).map(sign => `bg-zodiac-${sign}`));
    
    // Apply new class based on current view and selected sign
    if (currentView === 'personal' && yourSign && dailyRashiUpdate) {
      document.body.classList.add(`bg-zodiac-${yourSign}`);
    } else {
      // If not in personal view or no sign selected/daily update shown, clear all custom classes
      document.body.className = ''; 
    }
    // Cleanup function when component unmounts or dependencies change
    return () => {
      document.body.classList.remove(...Object.keys(rashiEmojis).map(sign => `bg-zodiac-${sign}`));
      document.body.className = ''; 
    };
  }, [currentView, yourSign, dailyRashiUpdate]);

  // CSS for background animation (add this to your global CSS, e.g., index.css or a style tag in App.js)
  // For demonstration, I'm including it as inline styles for some elements and adding classes dynamically.
  // You would typically define these in a separate CSS file.
  const animatedBgStyles = `
    @keyframes moveRashi {
      0% { background-position: 0% 0%; }
      100% { background-position: 100% 100%; }
    }

    body {
      transition: background-image 1s ease-in-out;
      background-size: 400px 400px; /* Increased size for prominent emoji */
      background-repeat: repeat;
      animation: moveRashi 70s infinite alternate linear; /* Adjusted animation for subtle yet noticeable effect */
      background-color: #f0f4f8; /* Fallback background color */
    }

    /* Dynamic classes for specific zodiac backgrounds */
    .bg-zodiac-ржорзЗрж╖ { background-image: url('data:image/svg+xml;charset=utf-8,<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24"><text x="50%" y="50%" dominant-baseline="middle" text-anchor="middle" font-size="24" fill="rgba(0,0,0,0.15)">тЩИ</text></svg>'); }
    .bg-zodiac-ржмрзГрж╖ { background-image: url('data:image/svg+xml;charset=utf-8,<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24"><text x="50%" y="50%" dominant-baseline="middle" text-anchor="middle" font-size="24" fill="rgba(0,0,0,0.15)">тЩЙ</text></svg>'); }
    .bg-zodiac-ржорж┐ржерзБржи { background-image: url('data:image/svg+xml;charset=utf-8,<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24"><text x="50%" y="50%" dominant-baseline="middle" text-anchor="middle" font-size="24" fill="rgba(0,0,0,0.15)">тЩК</text></svg>'); }
    .bg-zodiac-ржХрж░рзНржХржЯ { background-image: url('data:image/svg+xml;charset=utf-8,<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24"><text x="50%" y="50%" dominant-baseline="middle" text-anchor="middle" font-size="24" fill="rgba(0,0,0,0.15)">тЩЛ</text></svg>'); }
    .bg-zodiac-рж╕рж┐ржВрж╣ { background-image: url('data:image/svg+xml;charset=utf-8,<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24"><text x="50%" y="50%" dominant-baseline="middle" text-anchor="middle" font-size="24" fill="rgba(0,0,0,0.15)">тЩМ</text></svg>'); }
    .bg-zodiac-ржХржирзНржпрж╛ { background-image: url('data:image/svg+xml;charset=utf-8,<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24"><text x="50%" y="50%" dominant-baseline="middle" text-anchor="middle" font-size="24" fill="rgba(0,0,0,0.15)">тЩН</text></svg>'); }
    .bg-zodiac-рждрзБрж▓рж╛ { background-image: url('data:image/svg+xml;charset=utf-8,<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24"><text x="50%" y="50%" dominant-baseline="middle" text-anchor="middle" font-size="24" fill="rgba(0,0,0,0.15)">тЩО</text></svg>'); }
    .bg-zodiac-ржмрзГрж╢рзНржЪрж┐ржХ { background-image: url('data:image/svg+xml;charset=utf-8,<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24"><text x="50%" y="50%" dominant-baseline="middle" text-anchor="middle" font-size="24" fill="rgba(0,0,0,0.15)">тЩП</text></svg>'); }
    .bg-zodiac-ржзржирзБ { background-image: url('data:image/svg+xml;charset=utf-8,<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24"><text x="50%" y="50%" dominant-baseline="middle" text-anchor="middle" font-size="24" fill="rgba(0,0,0,0.15)">тЩР</text></svg>'); }
    .bg-zodiac-ржоржХрж░ { background-image: url('data:image/svg+xml;charset=utf-8,<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24"><text x="50%" y="50%" dominant-baseline="middle" text-anchor="middle" font-size="24" fill="rgba(0,0,0,0.15)">тЩС</text></svg>'); }
    .bg-zodiac-ржХрзБржорзНржн { background-image: url('data:image/svg+xml;charset=utf-8,<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24"><text x="50%" y="50%" dominant-baseline="middle" text-anchor="middle" font-size="24" fill="rgba(0,0,0,0.15)">тЩТ</text></svg>'); }
    .bg-zodiac-ржорзАржи { background-image: url('data:image/svg+xml;charset=utf-8,<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24"><text x="50%" y="50%" dominant-baseline="middle" text-anchor="middle" font-size="24" fill="rgba(0,0,0,0.15)">тЩУ</text></svg>'); }

    .input-background-animation {
      background-size: 70px 70px; /* Increased size for input fields */
      background-repeat: repeat;
      animation: moveRashi 40s infinite linear; /* Adjusted animation for input fields */
      transition: background-image 0.5s ease-in-out;
      position: relative; 
      z-index: 1; 
    }

    /* Specific background for input fields */
    .input-background-animation.rashi-your {
      background-image: url('data:image/svg+xml;charset=utf-8,<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24"><text x="50%" y="50%" dominant-baseline="middle" text-anchor="middle" font-size="28" fill="rgba(0,0,0,0.2)">${rashiEmojis[yourSign] || ''}</text></svg>');
    }
    .input-background-animation.rashi-partner {
      background-image: url('data:image/svg+xml;charset=utf-8,<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24"><text x="50%" y="50%" dominant-baseline="middle" text-anchor="middle" font-size="28" fill="rgba(0,0,0,0.2)">${rashiEmojis[partnerSign] || ''}</text></svg>');
    }
  `;

  // Dynamically inject styles into the head (for immediate effect in immersive preview)
  // In a real project, you'd put these in a .css file and import it.
  useEffect(() => {
    const styleSheet = document.createElement("style");
    styleSheet.type = "text/css";
    styleSheet.innerText = animatedBgStyles;
    document.head.appendChild(styleSheet);
    return () => {
      document.head.removeChild(styleSheet);
    };
  }, [animatedBgStyles]); // Re-inject if animatedBgStyles change (due to yourSign/partnerSign)


  const getDetailedMatchResult = (sign1, sign2) => {
    if (!sign1 || !sign2) {
      return {
        level: "ржЕрж╕ржорзНржкрзВрж░рзНржг рждржерзНржп",
        description: "ржжржпрж╝рж╛ ржХрж░рзЗ ржЙржнржпрж╝ рж░рж╛рж╢рж┐рж░ рждржерзНржп ржкрзВрж░ржг ржХрж░рзБржиред",
        strengths: "",
        challenges: ""
      };
    }

    const result = detailedCompatibilityData[sign1]?.[sign2];
    if (result) {
      return result;
    } else {
      // Fallback for undefined pairings (shouldn't happen with comprehensive data)
      return {
        level: "рж╕рж╛ржзрж╛рж░ржг ржорж┐рж▓",
        description: "ржПржЗ рж░рж╛рж╢рж┐рж░ ржЬрзБржЯрж┐рж░ ржЬржирзНржп ржмрж┐рж╕рзНрждрж╛рж░рж┐ржд рждржерзНржп ржЙржкрж▓ржмрзНржз ржирзЗржЗ, рждржмрзЗ рж╕ржорзНржкрж░рзНржХ ржЯрж┐ржХрж┐ржпрж╝рзЗ рж░рж╛ржЦрждрзЗ ржкрж░рж╕рзНржкрж░рзЗрж░ ржмрзЛржЭрж╛ржкржбрж╝рж╛ ржЦрзБржм ржЬрж░рзБрж░рж┐ рж╣ржмрзЗред",
        strengths: "ржЦрзЛрж▓рж╛ржорзЗрж▓рж╛ ржпрзЛржЧрж╛ржпрзЛржЧ ржПржмржВ ржкрж╛рж░рж╕рзНржкрж░рж┐ржХ рж╕ржорзНржорж╛ржи ржмржЬрж╛ржпрж╝ рж░рж╛ржЦрзБржиред",
        challenges: "ржнрж┐ржирзНржи рж╕рзНржмржнрж╛ржмрзЗрж░ ржХрж╛рж░ржгрзЗ ржХрж┐ржЫрзБ ржЪрзНржпрж╛рж▓рзЗржЮрзНржЬ ржЖрж╕рждрзЗ ржкрж╛рж░рзЗ, ржпрж╛ ржзрзИрж░рзНржп ржзрж░рзЗ ржорзЛржХрж╛ржмрж┐рж▓рж╛ ржХрж░рждрзЗ рж╣ржмрзЗред"
      };
    }
  };

  const checkMatch = () => {
    setMatchResult(getDetailedMatchResult(yourSign, partnerSign));
    // Check if palm images are provided for analysis notification
    if (yourPalmImages.length === 0 || partnerPalmImages.length === 0) {
      setPalmUploadPrompt("рж╣рж╛рждрзЗрж░ рж░рзЗржЦрж╛ ржмрж┐рж╢рзНрж▓рзЗрж╖ржгрзЗрж░ ржЬржирзНржп ржЖржкржирж╛рж░ ржПржмржВ ржЖржкржирж╛рж░ рж╕ржЩрзНржЧрзАрж░ рж╣рж╛рждрзЗрж░ рждрж╛рж▓рзБрж░ ржЫржмрж┐ ржЖржкрж▓рзЛржб ржХрж░рзБржиред");
    } else {
      setPalmUploadPrompt(""); // Clear prompt if images are present
    }
  };

  // Function to handle palm image upload and AI analysis
  const handlePalmImageUpload = async (e, isPartner = false) => {
    const files = Array.from(e.target.files); // Get all selected files
    const setImages = isPartner ? setPartnerPalmImages : setYourPalmImages;
    const setImageError = isPartner ? setPartnerPalmImageError : setYourPalmImageError;
    const setIsAnalyzing = isPartner ? setIsAnalyzingPartnerPalm : setIsAnalyzingYourPalm;
    const setAnalysisResult = isPartner ? setPartnerPalmAnalysisResult : setYourPalmAnalysisResult;

    setImageError("");
    setAnalysisResult(""); // Clear previous analysis result

    if (files.length === 0) {
      return;
    }

    if (files.length > 2) {
      setImageError("рж╕рж░рзНржмрзЛржЪрзНржЪ рзиржЯрж┐ ржЫржмрж┐ ржЖржкрж▓рзЛржб ржХрж░рж╛ ржпрж╛ржмрзЗред");
      // Clear current selection if more than 2 files are chosen
      e.target.value = '';
      return;
    }

    // Check file types
    for (const file of files) {
      if (!file.type.startsWith("image/")) {
        setImageError("рж╢рзБржзрзБржорж╛рждрзНрж░ ржЫржмрж┐рж░ ржлрж╛ржЗрж▓ ржЖржкрж▓рзЛржб ржХрж░рзБржиред");
        e.target.value = '';
        return;
      }
    }

    setIsAnalyzing(true); // Start loading indicator
    setImages([]); // Clear previous images before processing new ones

    let uploadedImageUrls = [];
    let analysisPromises = [];

    for (const file of files) {
      const reader = new FileReader();
      reader.onloadend = async () => {
        const base64ImageData = reader.result.split(',')[1]; // Get base64 string without data:image/png;base64, prefix
        uploadedImageUrls.push(URL.createObjectURL(file)); // Store URL for display

        const prompt = "ржПржЗ рж╣рж╛рждрзЗрж░ рждрж╛рж▓рзБрж░ ржЫржмрж┐ржЯрж┐ ржмрж┐рж╢рзНрж▓рзЗрж╖ржг ржХрж░рзЗ ржПрж░ ржкрзНрж░ржзрж╛ржи рж░рзЗржЦрж╛ржЧрзБрж▓рзЛ (ржпрзЗржоржи: ржЬрзАржмржи рж░рзЗржЦрж╛, рж╣рзГржжржпрж╝ рж░рзЗржЦрж╛, ржорж╕рзНрждрж┐рж╖рзНржХ рж░рзЗржЦрж╛) рж╕ржорзНржкрж░рзНржХрзЗ ржПржХржЯрж┐ рж╕ржВржХрзНрж╖рж┐ржкрзНржд ржЬрзНржпрзЛрждрж┐рж╖рж╢рж╛рж╕рзНрждрзНрж░рзАржпрж╝ ржмрзНржпрж╛ржЦрзНржпрж╛ ржжрж╛ржУ ржПржмржВ ржПрж░ рж╕ржорзНржнрж╛ржмрзНржп ржЕрж░рзНрже ржХрзА рж╣рждрзЗ ржкрж╛рж░рзЗ рждрж╛ ржмрж╛ржВрж▓рж╛рзЯ рж▓рзЗржЦрзЛред";
        let chatHistory = [];
        chatHistory.push({ role: "user", parts: [{ text: prompt }] });
        const payload = {
            contents: [
                  {
                      role: "user",
                      parts: [
                          { text: prompt },
                          {
                              inlineData: {
                                  mimeType: file.type, // Use actual file mime type
                                  data: base64ImageData
                              }
                          }
                      ]
                  }
              ],
          generationConfig: {
              responseMimeType: "application/json",
              responseSchema: {
                  type: "OBJECT",
                  properties: {
                      palmAnalysis: { type: "STRING" }
                  },
                  required: ["palmAnalysis"]
              }
          }
        };
        const apiKey = ""; // Canvas will automatically provide this in runtime
        const apiUrl = `https://generativelanguage.googleapis.com/v1beta/models/gemini-2.0-flash:generateContent?key=${apiKey}`;

        analysisPromises.push(
          fetch(apiUrl, {
                     method: 'POST',
                     headers: { 'Content-Type': 'application/json' },
                     body: JSON.stringify(payload)
                 })
          .then(response => {
            if (!response.ok) {
              return response.text().then(text => { throw new Error(`API Error: ${response.status} ${response.statusText} - ${text}`); });
            }
            return response.json();
          })
          .then(result => {
            if (result.candidates && result.candidates.length > 0 &&
                result.candidates[0].content && result.candidates[0].content.parts &&
                result.candidates[0].content.parts.length > 0) {
              const jsonString = result.candidates[0].content.parts[0].text;
              const parsedJson = JSON.parse(jsonString);
              return parsedJson.palmAnalysis ? parsedJson.palmAnalysis.trim() : '';
            } else {
              console.error("Unexpected API response structure:", result);
              return "ржмрж┐рж╢рзНрж▓рзЗрж╖ржг ржХрж░рж╛ рж╕ржорзНржнржм рж╣ржпрж╝ржирж┐ред";
            }
          })
          .catch(error => {
            console.error("Error during palm analysis API call:", error);
            return "ржмрж┐рж╢рзНрж▓рзЗрж╖ржг ржХрж░рждрзЗ ржЧрж┐рзЯрзЗ ржПржХржЯрж┐ рждрзНрж░рзБржЯрж┐ рж╣ржпрж╝рзЗржЫрзЗред";
          })
        );

        // If all files are read, process analyses
        if (uploadedImageUrls.length === files.length) {
          setImages(uploadedImageUrls); // Set images for display
          const allAnalysisResults = await Promise.all(analysisPromises);
          setAnalysisResult(allAnalysisResults.join("\n\n--- ржжрзНржмрж┐рждрзАрзЯ ржЫржмрж┐рж░ ржмрж┐рж╢рзНрж▓рзЗрж╖ржг ---\n\n")); // Join results
          setIsAnalyzing(false); // Stop loading indicator
        }
      };
      reader.readAsDataURL(file); // Read the file as a data URL
    }
  };

  // Function to clear palm images and analysis
  const clearPalmImages = (isPartner = false) => {
    const setImages = isPartner ? setPartnerPalmImages : setYourPalmImages;
    const setImageError = isPartner ? setPartnerPalmImageError : setYourPalmImageError;
    const setIsAnalyzing = isPartner ? setIsAnalyzingPartnerPalm : setIsAnalyzingYourPalm;
    const setAnalysisResult = isPartner ? setPartnerPalmAnalysisResult : setYourPalmAnalysisResult;

    setImages([]);
    setImageError("");
    setAnalysisResult("");
    setIsAnalyzing(false);
    // Optionally clear the file input as well
    const fileInput = document.getElementById(isPartner ? 'partner-palm-upload' : 'your-palm-upload');
    if (fileInput) fileInput.value = '';
  };

  // Function to fetch daily rashi update from AI
  const fetchDailyRashiUpdate = async () => {
    if (!yourSign) {
      setDailyRashiError("ржжржпрж╝рж╛ ржХрж░рзЗ ржЖржкржирж╛рж░ рж░рж╛рж╢рж┐ ржирж┐рж░рзНржмрж╛ржЪржи ржХрж░рзБржи ржжрзИржирж┐ржХ рж░рж╛рж╢рж┐ржлрж▓ ржжрзЗржЦрж╛рж░ ржЬржирзНржпред");
      return;
    }
    setDailyRashiUpdate("");
    setDailyRashiError("");
    setIsFetchingDailyRashi(true);

    const prompt = `ржЖржЬржХрзЗрж░ ржЬржирзНржп ${yourSign} рж░рж╛рж╢рж┐рж░ ржПржХржЯрж┐ рж╕ржВржХрзНрж╖рж┐ржкрзНржд ржжрзИржирж┐ржХ рж░рж╛рж╢рж┐ржлрж▓ ржмрж╛ржВрж▓рж╛ржпрж╝ ржжрж╛ржУред ржПржЯрж┐ ржкрзНрж░рзЗржо, ржХрж░рзНржоржЬрзАржмржи, рж╕рзНржмрж╛рж╕рзНржерзНржп ржПржмржВ ржЖрж░рзНржерж┐ржХ ржжрж┐ржХ рж╕ржорзНржкрж░рзНржХрзЗ рж╕ржВржХрзНрж╖рзЗржкрзЗ рж▓рж┐ржЦржмрзЗред`;
    let chatHistory = [];
    chatHistory.push({ role: "user", parts: [{ text: prompt }] });
    const payload = {
      contents: chatHistory,
      generationConfig: {
        responseMimeType: "application/json",
        responseSchema: {
          type: "OBJECT",
          properties: {
            dailyHoroscope: { type: "STRING" }
          },
          required: ["dailyHoroscope"]
        }
      }
    };
    const apiKey = ""; // Canvas will automatically provide this in runtime
    const apiUrl = `https://generativelanguage.googleapis.com/v1beta/models/gemini-2.0-flash:generateContent?key=${apiKey}`;

    try {
      const response = await fetch(apiUrl, {
        method: 'POST',
        headers: { 'Content-Type': 'application/json' },
        body: JSON.stringify(payload)
      });

      if (!response.ok) {
        const errorText = await response.text();
        throw new Error(`API Error: ${response.status} ${response.statusText} - ${errorText}`);
      }

      const result = await response.json();
      console.log("Daily Rashi Raw Response:", result); // Log raw response for debugging

      if (result.candidates && result.candidates.length > 0 &&
          result.candidates[0].content && result.candidates[0].content.parts &&
          result.candidates[0].content.parts.length > 0) {
        const jsonString = result.candidates[0].content.parts[0].text;
        const parsedJson = JSON.parse(jsonString);
        const horoscopeText = parsedJson.dailyHoroscope ? parsedJson.dailyHoroscope.trim() : '';
        setDailyRashiUpdate(horoscopeText);
        // Send a test notification if enabled after fetching daily rashi
        if (notificationEnabled) {
          sendTestNotification(yourSign);
        }
      } else {
        setDailyRashiError("ржжрзИржирж┐ржХ рж░рж╛рж╢рж┐ржлрж▓ ржЖржирж╛ рж╕ржорзНржнржм рж╣ржпрж╝ржирж┐ред");
        console.error("Unexpected API response for daily rashi:", result);
      }
    } catch (error) {
      setDailyRashiError("ржжрзИржирж┐ржХ рж░рж╛рж╢рж┐ржлрж▓ ржЖржирждрзЗ ржЧрж┐рзЯрзЗ ржПржХржЯрж┐ рждрзНрж░рзБржЯрж┐ рж╣ржпрж╝рзЗржЫрзЗред");
      console.error("Error fetching daily rashi:", error);
    } finally {
      setIsFetchingDailyRashi(false);
    }
  };

  // Function to calculate zodiac sign from birth details using AI
  const calculateZodiacSignFromBirthDetails = async () => {
    if (!yourBirthDate || !yourBirthTime || !yourBirthPlace) {
      setSignCalculationError("рж░рж╛рж╢рж┐ ржмрзЗрж░ ржХрж░рж╛рж░ ржЬржирзНржп ржЬржирзНржо рждрж╛рж░рж┐ржЦ, рж╕ржоржпрж╝ ржПржмржВ рж╕рзНржерж╛ржи ржкрзВрж░ржг ржХрж░рзБржиред");
      return;
    }
    setSignCalculationError("");
    setIsCalculatingSign(true);
    setYourSign(""); // Clear previous sign

    // Refined prompt for AI: explicitly ask for only the Bengali name of the sign in JSON format
    const prompt = `Given the birth date: ${yourBirthDate}, birth time: ${yourBirthTime}, birth place: ${yourBirthPlace}. Determine the zodiac sign (рж░рж╛рж╢рж┐) in Bengali. Respond ONLY with a JSON object like {\"zodiacSign\": \"<Bengali Zodiac Sign>\"}. For example, if the sign is Aries, output {\"zodiacSign\": \"ржорзЗрж╖\"}. Do not include any other text, explanations, or conversational remarks outside this JSON.`;
    let chatHistory = [];
    chatHistory.push({ role: "user", parts: [{ text: prompt }] });
    const payload = {
      contents: chatHistory,
      generationConfig: {
        responseMimeType: "application/json",
        responseSchema: {
          type: "OBJECT",
          properties: {
            zodiacSign: { type: "STRING" }
          },
          required: ["zodiacSign"]
        }
      }
    };
    const apiKey = ""; // Canvas will automatically provide this in runtime
    const apiUrl = `https://generativelanguage.googleapis.com/v1beta/models/gemini-2.0-flash:generateContent?key=${apiKey}`;

    try {
      const response = await fetch(apiUrl, {
        method: 'POST',
        headers: { 'Content-Type': 'application/json' },
        body: JSON.stringify(payload)
      });

      // Check if the response was successful (HTTP status 200-299)
      if (!response.ok) {
        const errorText = await response.text(); // Get raw text for debugging
        throw new Error(`API Error: ${response.status} ${response.statusText} - ${errorText}`);
      }

      const result = await response.json();
      console.log("AI Raw Response (Sign Calculation):", result); // Log raw response for debugging

      if (result.candidates && result.candidates.length > 0 &&
          result.candidates[0].content && result.candidates[0].content.parts &&
          result.candidates[0].content.parts.length > 0) {
        const jsonString = result.candidates[0].content.parts[0].text;
        const parsedJson = JSON.parse(jsonString); // Parse the JSON string

        const predictedSign = parsedJson.zodiacSign ? parsedJson.zodiacSign.trim() : '';

        // Basic validation to check if the response is a known sign
        if (Object.keys(compatibility).includes(predictedSign)) {
          setYourSign(predictedSign);
          setSignCalculationError(""); // Clear any previous error
        } else {
          setSignCalculationError("ржЖржкржирж╛рж░ рж░рж╛рж╢рж┐ ржмрзЗрж░ ржХрж░рж╛ рж╕ржорзНржнржм рж╣ржпрж╝ржирж┐ред ржЕржирзБржЧрзНрж░рж╣ ржХрж░рзЗ рж╕ржарж┐ржХ рждржерзНржп ржжрж┐ржи ржмрж╛ ржорзНржпрж╛ржирзБржпрж╝рж╛рж▓рж┐ рж░рж╛рж╢рж┐ ржирж┐рж░рзНржмрж╛ржЪржи ржХрж░рзБржиред (AI ржерзЗржХрзЗ ржЕржкрзНрж░рждрзНржпрж╛рж╢рж┐ржд ржкрзНрж░рждрж┐ржХрзНрж░рж┐ржпрж╝рж╛)");
          console.error("AI returned an unexpected sign format or value:", predictedSign, parsedJson);
        }
      } else {
        setSignCalculationError("рж░рж╛рж╢рж┐ ржмрзЗрж░ ржХрж░рж╛ рж╕ржорзНржнржм рж╣ржпрж╝ржирж┐ред ржЕржирзБржЧрзНрж░рж╣ ржХрж░рзЗ ржЖржмрж╛рж░ ржЪрзЗрж╖рзНржЯрж╛ ржХрж░рзБржиред (API ржкрзНрж░рждрж┐ржХрзНрж░рж┐ржпрж╝рж╛ ржЕрж╕ржорзНржкрзВрж░рзНржг)");
        console.error("Unexpected API response structure for sign calculation:", result);
      }
    } catch (error) {
      setSignCalculationError("рж░рж╛рж╢рж┐ ржмрзЗрж░ ржХрж░рждрзЗ ржЧрж┐рзЯрзЗ ржПржХржЯрж┐ рждрзНрж░рзБржЯрж┐ рж╣ржпрж╝рзЗржЫрзЗред");
      console.error("Error during sign calculation API call:", error);
    } finally {
      setIsCalculatingSign(false);
    }
  };


  return (
    <div className="max-w-md mx-auto p-4 space-y-4 font-inter bg-gradient-to-br from-blue-50 to-purple-50 min-h-screen relative overflow-hidden">
      <h1 className="text-3xl font-extrabold text-center text-gray-900 mb-6 drop-shadow-sm">
        <span className="text-blue-600">рж░рж╛рж╢рж┐</span> ржУ <span className="text-purple-600">рж╣рж╛рждрзЗрж░ рж░рзЗржЦрж╛</span> ржмрж┐рж╢рзНрж▓рзЗрж╖ржг
      </h1>

      {/* Navigation Tabs */}
      <div className="flex justify-center space-x-4 mb-6">
        <Button
          onClick={() => setCurrentView('matchmaking')}
          className={`px-6 py-2 rounded-full text-lg font-semibold transition-all duration-300 ${
            currentView === 'matchmaking'
              ? 'bg-blue-600 text-white shadow-lg transform scale-105'
              : 'bg-gray-200 text-gray-700 hover:bg-gray-300'
          }`}
        >
          ржкрзНрж░рзЗржорзЗрж░ ржорж┐рж▓
        </Button>
        <Button
          onClick={() => setCurrentView('personal')}
          className={`px-6 py-2 rounded-full text-lg font-semibold transition-all duration-300 ${
            currentView === 'personal'
              ? 'bg-purple-600 text-white shadow-lg transform scale-105'
              : 'bg-gray-200 text-gray-700 hover:bg-gray-300'
          }`}
        >
          ржЖржорж╛рж░ рж░рж╛рж╢рж┐ржлрж▓
        </Button>
      </div>

      {currentView === 'matchmaking' && (
        <Card className="rounded-2xl shadow-xl border border-gray-200 bg-white p-6 relative z-10"> {/* Added z-10 to keep content above body background */}
          <CardContent className="space-y-6 pt-0"> {/* Adjusted padding */}
            {/* Your Details Section */}
            <h2 className="text-2xl font-bold text-gray-800 border-b-2 border-blue-200 pb-2 mb-4">ржЖржкржирж╛рж░ ржмрж┐ржмрж░ржг</h2>
            <div className={`relative p-3 rounded-lg overflow-hidden ${yourSign ? 'input-background-animation rashi-your' : ''}`}>
              <label className="block text-sm font-medium text-gray-700 mb-1">ржЖржкржирж╛рж░ рж░рж╛рж╢рж┐:</label>
              <Select onValueChange={setYourSign} value={yourSign}>
                <SelectTrigger className="w-full rounded-lg border border-gray-300 px-4 py-2 text-gray-700 shadow-sm focus:border-blue-500 focus:ring-blue-500">
                  <SelectValue placeholder="рж░рж╛рж╢рж┐ ржирж┐рж░рзНржмрж╛ржЪржи ржХрж░рзБржи" />
                </SelectTrigger>
                <SelectContent className="rounded-lg shadow-lg border border-gray-200 bg-white z-10">
                  {Object.keys(compatibility).map((sign) => (
                    <SelectItem key={sign} value={sign} className="px-4 py-2 hover:bg-gray-100 cursor-pointer">
                      {sign}
                    </SelectItem>
                  ))}
                </SelectContent>
              </Select>
            </div>

            <div>
              <label className="block text-sm font-medium text-gray-700 mb-1">ржЬржирзНржо рждрж╛рж░рж┐ржЦ:</label>
              <Input
                type="date"
                value={yourBirthDate}
                onChange={(e) => setYourBirthDate(e.target.value)}
                className="w-full p-3 border border-gray-300 rounded-lg focus:border-blue-500 focus:ring-blue-500"
              />
            </div>

            <div>
              <label className="block text-sm font-medium text-gray-700 mb-1">ржЬржирзНржо рж╕ржоржпрж╝:</label>
              <Input
                type="time"
                value={yourBirthTime}
                onChange={(e) => setYourBirthTime(e.target.value)}
                className="w-full p-3 border border-gray-300 rounded-lg focus:border-blue-500 focus:ring-blue-500"
              />
